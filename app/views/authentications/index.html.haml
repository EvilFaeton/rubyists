- title "Authentications"

%table
  %tr
    %th User
  - for authentication in @authentications
    %tr
      %td= authentication.user_id
      %td= link_to 'Show', authentication
      %td= link_to 'Edit', edit_authentication_path(authentication)
      %td= link_to 'Destroy', authentication, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Authentication", new_authentication_path(authentication)
